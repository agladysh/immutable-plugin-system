{
  "$schema": "https://typedoc.org/schema.json",
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-coverage"],
  "cleanOutputDir": true,
  "tsconfig": "./tsconfig.build.json",
  "entryPoints": ["./src/index.ts"],
  "highlightLanguages": [
    "bash",
    "console",
    "css",
    "diff",
    "html",
    "javascript",
    "json",
    "jsonc",
    "json5",
    "tsx",
    "typescript"
  ],
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "jsDocCompatibility": false,
  "validation": {
    "notDocumented": true,
    "notExported": true,
    "invalidLink": true,
    "rewrittenLink": true,
    "unusedMergeModuleWith": true
  },
  "intentionallyNotExported": [
    "_NormalizedString",
    "_StringPart",
    "_NonStringPart"
  ],
  "treatValidationWarningsAsErrors": true,
  "treatWarningsAsErrors": true,
  "readme": "none",
  "gitRevision": "main",
  "outputs": [
    {
      "name": "html",
      "path": "./site/",
      "options": {
        "readme": "README.md",
        "projectDocuments": ["./docs/spec.md"],
        "githubPages": true
      }
    },
    {
      "name": "markdown",
      "path": "./docs/api/",
      "options": {
        "githubPages": false,
        "hidePageHeader": true,
        "router": "member"
      }
    }
  ]
}
